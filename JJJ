<script>
  function sizeIframe(){
    var h = $(window).height();
    var nav_h = $('.site_header_area').height()
    var more_h = $('.more_bar').height()
    // var dash_h = $('.dashboard').height()
    $('section.iframe_media_feature').height(h - nav_h - more_h)
    // now relying on css in _primary_media_feature.scss to set height of iframe_media_feature iframe.
    // setting it here with js is nearly impossible as the height of the dashboard is not reliable.
    // this is because a nested set time out functions in solar_system_dashboard.js designed to work
    // around a slick slider bug for android that lasts for a total of 3 seconds is "refreshing"
    // slick slider - smb 04-08-2020
    // $('section.iframe_media_feature iframe').height(h - nav_h - more_h - dash_h)
  }
  $(document).ready(function(){
    window.setTimeout(function(){
      $('body').addClass('more_bar_present');
      sizeIframe();
    }, 30)
  })
  $(window).on('resize', sizeIframe)
</script>

